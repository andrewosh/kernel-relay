{
  "name": "kernel-relay",
  "version": "0.0.1",
  "description": "websocket-zmq relay for Jupyter kernels ",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "mkdirp lib && npm run clean",
    "build": "npm run build:es5",
    "build:es5": "babel src --out-dir lib/ --source-maps",
    "clean": "rimraf lib/*"
  },
  "bin": {
    "kernel-relay": "./src/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nteract/kernel-relay.git"
  },
  "keywords": [
    "websocket",
    "zmq",
    "jupyter",
    "kernel",
    "nteract"
  ],
  "author": "nteract contributors",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/nteract/kernel-relay/issues"
  },
  "homepage": "https://github.com/nteract/kernel-relay#readme",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "babel-register": "^6.5.2",
    "chalk": "^1.1.1",
    "enchannel-zmq-backend": "^1.0.1",
    "express": "^4.13.4",
    "kernelspecs": "^1.0.1",
    "node-uuid": "^1.4.7",
    "socket.io": "^1.4.5",
    "spawnteract": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-eslint": "^4.1.6",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-export-extensions": "^6.3.13",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.3.13"
  }
